<%= link_to "All", incomes_path(month: '0'), class: 'btn btn-default'%>
<%= link_to "Jan", incomes_path(month: '1'), class: 'btn btn-default'%>
<%= link_to "Feb", incomes_path(month: '2'), class: 'btn btn-default' %>
<%= link_to "Mar", incomes_path(month: '3'), class: 'btn btn-default' %>
<%= link_to "Apr", incomes_path(month: '4'), class: 'btn btn-default' %>
<%= link_to "May", incomes_path(month: '5'), class: 'btn btn-default' %>
<%= link_to "Jun", incomes_path(month: '6'), class: 'btn btn-default' %>
<%= link_to "Jul", incomes_path(month: '7'), class: 'btn btn-default' %>
<%= link_to "Aug", incomes_path(month: '8'), class: 'btn btn-default' %>
<%= link_to "Sep", incomes_path(month: '9'), class: 'btn btn-default' %>
<%= link_to "Oct", incomes_path(month: '10'), class: 'btn btn-default' %>
<%= link_to "Nov", incomes_path(month: '11'), class: 'btn btn-default' %>
<%= link_to "Dec", incomes_path(month: '12'), class: 'btn btn-default' %>
<% if params[:month].to_i == 0 || params[:month] == nil %>
  <i class="month-name">- All</i>
<% else %>
  <i class="month-name">- <%= Date::MONTHNAMES[params[:month].to_i] %></i>
<% end %>